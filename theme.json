{
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#444444",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#003c71",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#71003c",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#0057a4",
					"name": "Section",
					"slug": "section"
				}
			]
		},
		"layout": {
			"contentSize": "50rem",
			"wideSize": "62.5rem"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "\"Open Sans\"",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/open-sans/open-sans-400-normal.woff2"
							]
						},
						{
							"fontFamily": "\"Open Sans\"",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/open-sans/open-sans-700-normal.woff2"
							]
						}
					],
					"fontFamily": "\"Open Sans\", sans-serif",
					"name": "Open Sans",
					"slug": "open-sans"
				},
				{
					"fontFace": [
						{
							"fontFamily": "\"PT Serif\"",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/pt-serif/pt-serif-400-normal.woff2"
							]
						},
						{
							"fontFamily": "\"PT Serif\"",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/pt-serif/pt-serif-700-normal.woff2"
							]
						}
					],
					"fontFamily": "\"PT Serif\", serif",
					"name": "PT Serif",
					"slug": "pt-serif"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "1.13rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "1.5rem",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "2rem",
					"slug": "x-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/archives": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--open-sans)"
				}
			},
			"core/button": {
				"border": {
					"bottom": {
						"color": "var(--wp--preset--color--secondary)",
						"style": "solid",
						"width": "2px"
					},
					"left": {
						"color": "var(--wp--preset--color--secondary)",
						"style": "solid",
						"width": "2px"
					},
					"right": {
						"color": "var(--wp--preset--color--secondary)",
						"style": "solid",
						"width": "2px"
					},
					"top": {
						"color": "var(--wp--preset--color--secondary)",
						"style": "solid",
						"width": "2px"
					}
				},
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--base)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--base)"
						}
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--open-sans)",
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"variations": {
					"outline": {
						"border": {
							"bottom": {
								"color": "var(--wp--preset--color--secondary)",
								"style": "solid",
								"width": "1px"
							},
							"left": {
								"color": "var(--wp--preset--color--secondary)",
								"style": "solid",
								"width": "1px"
							},
							"radius": "0px",
							"right": {
								"color": "var(--wp--preset--color--secondary)",
								"style": "solid",
								"width": "1px"
							},
							"top": {
								"color": "var(--wp--preset--color--secondary)",
								"style": "solid",
								"width": "1px"
							}
						},
						"color": {
							"background": "var(--wp--preset--color--secondary)",
							"text": "var(--wp--preset--color--base)"
						},
						"shadow": "none",
						"typography": {
							"fontFamily": "var(--wp--preset--font-family--open-sans)",
							"fontSize": "var(--wp--preset--font-size--small)"
						}
					}
				}
			},
			"core/buttons": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--open-sans)"
				}
			},
			"core/heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--pt-serif)"
				}
			},
			"core/list": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--open-sans)"
				}
			},
			"core/paragraph": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--open-sans)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/pullquote": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--open-sans)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/quote": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--open-sans)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--base)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--open-sans)",
					"textTransform": "uppercase"
				}
			},
			"caption": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--pt-serif)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "3rem"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "2.25rem"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "1.75rem"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "1.5rem"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "1.25rem"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "1rem"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--pt-serif)",
					"fontStyle": "normal",
					"fontWeight": "700"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--section)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--open-sans)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			}
		},
		"spacing": {
			"padding": {
				"bottom": "0px",
				"left": "1rem",
				"right": "1rem",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--pt-serif)",
			"fontStyle": "normal",
			"fontWeight": "400"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json"
}